{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "@please-practice/gohello#start": {
      "cache": false
    },
    "gohello": {
      "dependsOn": ["@please-practice/gohello#start"]
    },
    "@please-practice/jshello#start": {
      "cache": false
    },
    "jshello": {
      "dependsOn": ["@please-practice/jshello#start"]
    },
    "@please-practice/pyhello#start": {
      "cache": false
    },
    "pyhello": {
      "dependsOn": ["@please-practice/pyhello#start"]
    },
    "hello": {
      "dependsOn": ["gohello", "jshello", "pyhello"]
    }
  }
}
